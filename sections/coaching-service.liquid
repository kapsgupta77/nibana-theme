 {%- comment -%} MYTHS / TRUTHS — Before → After compare rows {%- endcomment -%}
{%- liquid
  assign myths_raw  = service.myths  | append: ''
  assign truths_raw = service.truths | append: ''

  assign br = '<br />'

  assign myths_html  = myths_raw  | newline_to_br
  assign truths_html = truths_raw | newline_to_br

  assign myths_norm  = myths_html  | replace: '<br/>', br | replace: '<br>', br | replace: '&nbsp;',' '
  assign truths_norm = truths_html | replace: '<br/>', br | replace: '<br>', br | replace: '&nbsp;',' '

  assign myths_pipe  = myths_norm  | replace: br, '|||'
  assign truths_pipe = truths_norm | replace: br, '|||'

  assign myths  = myths_pipe  | split: '|||'
  assign truths = truths_pipe | split: '|||'

  assign myths_count  = 0
  for m in myths
    assign t = m | strip
    if t != '' 
      assign myths_count = myths_count | plus: 1
    endif
  endfor

  assign truths_count = 0
  for t in truths
    assign z = t | strip
    if z != '' 
      assign truths_count = truths_count | plus: 1
    endif
  endfor

  assign pair_count = 0
  if myths_count > 0 and truths_count > 0
    assign pair_count = myths_count
    if truths_count < pair_count
      assign pair_count = truths_count
    endif
  endif
-%}

{%- if myths_count > 0 or truths_count > 0 -%}
<style>
  /* ===== Compare Rows (scoped) ===== */
  .nb-compare{
    --cmp-surface:#fff;
    --cmp-accent-myth:#B67955;   /* warm pebble/chocolate tint */
