        {%- assign body = entry | remove_first: heading -%}
        {%- assign body = body | remove_first: ' - ' | strip -%}
      {%- elsif entry contains '-' -%}
        {%- assign heading = entry | split: '-' | first | strip -%}
        {%- assign body = entry | remove_first: heading -%}
