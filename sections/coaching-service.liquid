    box-shadow: 0 6px 14px rgba(0,0,0,.05);
  }
  .nb-outcomes__grid{
    grid-template-columns: 1fr;
    gap: 8px;
  }
  .nb-outcome{
    padding: 12px 12px 12px 16px;
