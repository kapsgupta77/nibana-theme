{% doc %}
  @prompt
    Create a responsive image slider with text overlay that shows 4 slides per view on desktop and 1 slide on mobile. Include navigation arrows positioned in the top right corner, section title in the top left corner, and advance by 4 slides on desktop (1 on mobile) when arrows are clicked. Each slide should be linkable and include comprehensive customization options for colors, spacing, typography, slider behavior, and layout settings.
    
    I want to add slide dynamically

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-image-slider-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    background-color: {{ block.settings.background_color }};
    padding: {{ block.settings.section_padding_top }}px 0 {{ block.settings.section_padding_bottom }}px;
  }

  .ai-image-slider-container-{{ ai_gen_id }} {
    margin: 0 auto;
    padding: 0 20px;
  }

  .ai-image-slider-header-{{ ai_gen_id }} {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: {{ block.settings.title_spacing }}px;
  }

  .ai-image-slider-title-{{ ai_gen_id }} {
    color: {{ block.settings.title_color }};
    font-size: {{ block.settings.title_size }}px;
    margin: 0;
    font-weight: {{ block.settings.title_weight }};
  }

  .ai-image-slider-nav-{{ ai_gen_id }} {
    display: flex;
    gap: 8px;
  }

  .ai-image-slider-nav-btn-{{ ai_gen_id }} {
    width: {{ block.settings.nav_button_size }}px;
    height: {{ block.settings.nav_button_size }}px;
    border: none;
    border-radius: {{ block.settings.nav_button_radius }}px;
    background-color: {{ block.settings.nav_button_color }};
    color: {{ block.settings.nav_button_icon_color }};
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: all 0.3s ease;
  }

  .ai-image-slider-nav-btn-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.nav_button_hover_color }};
    color: {{ block.settings.nav_button_hover_icon_color }};
  }

  .ai-image-slider-nav-btn-{{ ai_gen_id }}:disabled {
    opacity: 0.5;
    cursor: not-allowed;
  }

  .ai-image-slider-track-{{ ai_gen_id }} {
    overflow: hidden;
    border-radius: {{ block.settings.slider_border_radius }}px;
  }

  .ai-image-slider-slides-{{ ai_gen_id }} {
    display: flex;
    transition: transform 0.5s ease;
    gap: {{ block.settings.slide_gap }}px;
  }

  .ai-image-slider-slide-{{ ai_gen_id }} {
    position: relative;
    flex: 0 0 calc((100% - (3 * {{ block.settings.slide_gap }}px)) / 4);
    aspect-ratio: {{ block.settings.slide_aspect_ratio }};
    border-radius: {{ block.settings.slide_border_radius }}px;
    overflow: hidden;
    background-color: #f4f4f4;
  }

  .ai-image-slider-slide-link-{{ ai_gen_id }} {
    display: block;
    width: 100%;
    height: 100%;
    position: relative;
    text-decoration: none;
  }

  .ai-image-slider-slide-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-image-slider-slide-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #f4f4f4;
    position: relative;
  }

  .ai-image-slider-slide-placeholder-{{ ai_gen_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  .ai-image-slider-slide-empty-state-{{ ai_gen_id }} {
    position: absolute;
    bottom: 10px;
    left: 50%;
    transform: translateX(-50%);
    font-size: 12px;
    color: #666;
    text-align: center;
    pointer-events: none;
  }

  .ai-image-slider-slide-overlay-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: linear-gradient(
      {{ block.settings.overlay_direction }},
      {{ block.settings.overlay_color_start }},
      {{ block.settings.overlay_color_end }}
    );
    opacity: {{ block.settings.overlay_opacity | divided_by: 100.0 }};
    display: flex;
    align-items: {{ block.settings.text_vertical_alignment }};
    justify-content: {{ block.settings.text_horizontal_alignment }};
    padding: {{ block.settings.text_padding }}px;
  }

  .ai-image-slider-slide-content-{{ ai_gen_id }} {
    text-align: {{ block.settings.text_alignment }};
    color: {{ block.settings.text_color }};
  }

  .ai-image-slider-slide-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.slide_title_size }}px;
    font-weight: {{ block.settings.slide_title_weight }};
    margin: 0 0 {{ block.settings.title_bottom_spacing }}px 0;
    line-height: 1.2;
  }

  .ai-image-slider-slide-description-{{ ai_gen_id }} {
    font-size: {{ block.settings.slide_description_size }}px;
    margin: 0;
    line-height: 1.4;
  }

  @media screen and (max-width: 768px) {
    .ai-image-slider-slide-{{ ai_gen_id }} {
      flex: 0 0 100%;
    }

    .ai-image-slider-header-{{ ai_gen_id }} {
      flex-direction: column;
      align-items: flex-start;
      gap: 16px;
    }

    .ai-image-slider-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.8 }}px;
    }

    .ai-image-slider-slide-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.slide_title_size | times: 0.8 }}px;
    }

    .ai-image-slider-slide-description-{{ ai_gen_id }} {
      font-size: {{ block.settings.slide_description_size | times: 0.9 }}px;
    }
  }
{% endstyle %}

<image-slider-{{ ai_gen_id }}
  class="ai-image-slider-{{ ai_gen_id }}"
  data-slides-desktop="4"
  data-slides-mobile="1"
  {{ block.shopify_attributes }}
>
  <div class="ai-image-slider-container-{{ ai_gen_id }}">
    <div class="ai-image-slider-header-{{ ai_gen_id }}">
      {% if block.settings.section_title != blank %}
        <h2 class="ai-image-slider-title-{{ ai_gen_id }}">{{ block.settings.section_title }}</h2>
      {% endif %}
      
      <div class="ai-image-slider-nav-{{ ai_gen_id }}">
        <button
          class="ai-image-slider-nav-btn-{{ ai_gen_id }} ai-image-slider-prev-{{ ai_gen_id }}"
          aria-label="Previous slides"
        >
          <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <polyline points="15,18 9,12 15,6"></polyline>
          </svg>
        </button>
        <button
          class="ai-image-slider-nav-btn-{{ ai_gen_id }} ai-image-slider-next-{{ ai_gen_id }}"
          aria-label="Next slides"
        >
          <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <polyline points="9,18 15,12 9,6"></polyline>
          </svg>
        </button>
      </div>
    </div>

    <div class="ai-image-slider-track-{{ ai_gen_id }}">
      <div class="ai-image-slider-slides-{{ ai_gen_id }}">
        {% for i in (1..20) %}
          {% liquid
            assign slide_enabled_key = 'slide_' | append: i | append: '_enabled'
            assign slide_image_key = 'slide_' | append: i | append: '_image'
            assign slide_title_key = 'slide_' | append: i | append: '_title'
            assign slide_description_key = 'slide_' | append: i | append: '_description'
            assign slide_link_key = 'slide_' | append: i | append: '_link'

            assign slide_enabled = block.settings[slide_enabled_key]
            assign slide_image = block.settings[slide_image_key]
            assign slide_title = block.settings[slide_title_key]
            assign slide_description = block.settings[slide_description_key]
            assign slide_link = block.settings[slide_link_key]
          %}

          {% if slide_enabled %}
            <div class="ai-image-slider-slide-{{ ai_gen_id }}">
              {% if slide_link != blank %}
                <a href="{{ slide_link }}" class="ai-image-slider-slide-link-{{ ai_gen_id }}">
              {% else %}
                <div class="ai-image-slider-slide-link-{{ ai_gen_id }}">
              {% endif %}
                
                {% if slide_image %}
                  <img
                    src="{{ slide_image | image_url: width: 600 }}"
                    alt="{{ slide_image.alt | escape }}"
                    class="ai-image-slider-slide-image-{{ ai_gen_id }}"
                    loading="lazy"
                    width="600"
                    height="400"
                  >
                {% else %}
                  <div class="ai-image-slider-slide-placeholder-{{ ai_gen_id }}">
                    {{ 'image' | placeholder_svg_tag }}
                    <div class="ai-image-slider-slide-empty-state-{{ ai_gen_id }}">
                      Add image for slide {{ i }}
                    </div>
                  </div>
                {% endif %}

                {% if slide_title != blank or slide_description != blank %}
                  <div class="ai-image-slider-slide-overlay-{{ ai_gen_id }}">
                    <div class="ai-image-slider-slide-content-{{ ai_gen_id }}">
                      {% if slide_title != blank %}
                        <h3 class="ai-image-slider-slide-title-{{ ai_gen_id }}">{{ slide_title }}</h3>
                      {% endif %}
                      {% if slide_description != blank %}
                        <p class="ai-image-slider-slide-description-{{ ai_gen_id }}">{{ slide_description }}</p>
                      {% endif %}
                    </div>
                  </div>
                {% endif %}

              {% if slide_link != blank %}
                </a>
              {% else %}
                </div>
              {% endif %}
            </div>
          {% endif %}
        {% endfor %}
      </div>
    </div>
  </div>
</image-slider-{{ ai_gen_id }}>

<script>
  (function() {
    class ImageSlider{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentIndex = 0;
        this.slidesPerView = 4;
        this.slidesPerViewMobile = 1;
      }

      connectedCallback() {
        this.slidesContainer = this.querySelector('.ai-image-slider-slides-{{ ai_gen_id }}');
        this.slides = this.querySelectorAll('.ai-image-slider-slide-{{ ai_gen_id }}');
        this.prevBtn = this.querySelector('.ai-image-slider-prev-{{ ai_gen_id }}');
        this.nextBtn = this.querySelector('.ai-image-slider-next-{{ ai_gen_id }}');
        
        this.totalSlides = this.slides.length;
        this.updateSlidesPerView();
        this.setupEventListeners();
        this.updateSlider();
        
        window.addEventListener('resize', () => {
          this.updateSlidesPerView();
          this.updateSlider();
        });
      }

      updateSlidesPerView() {
        this.slidesPerView = window.innerWidth <= 768 ? this.slidesPerViewMobile : 4;
      }

      setupEventListeners() {
        this.prevBtn.addEventListener('click', () => this.goToPrevious());
        this.nextBtn.addEventListener('click', () => this.goToNext());}

      goToPrevious() {
        this.currentIndex = Math.max(0, this.currentIndex - this.slidesPerView);
        this.updateSlider();
      }

      goToNext() {
        const maxIndex = Math.max(0, this.totalSlides - this.slidesPerView);
        this.currentIndex = Math.min(maxIndex, this.currentIndex + this.slidesPerView);
        this.updateSlider();
      }

      updateSlider() {
        const slideWidth = 100 / this.slidesPerView;
        const translateX = -(this.currentIndex * slideWidth);
        
        this.slidesContainer.style.transform = `translateX(${translateX}%)`;
        
        const maxIndex = Math.max(0, this.totalSlides - this.slidesPerView);
        this.prevBtn.disabled = this.currentIndex === 0;
        this.nextBtn.disabled = this.currentIndex >= maxIndex;
      }
    }

    customElements.define('image-slider-{{ ai_gen_id }}', ImageSlider{{ ai_gen_id }});
  })();
</script>

