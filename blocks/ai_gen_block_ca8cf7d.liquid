{% doc %}
  @prompt
    create testimonial page section in grid view for consultancy service need 30 blocks for reviews

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-testimonial-grid-{{ ai_gen_id }} {
    padding: {{ block.settings.section_padding_top }}px 0 {{ block.settings.section_padding_bottom }}px;
    background-color: {{ block.settings.background_color }};
  }

  .ai-testimonial-container-{{ ai_gen_id }} {
    max-width:1200px;
    margin: 0 auto;
    padding: 0 20px;
  }

  .ai-testimonial-header-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 40px;
  }

  .ai-testimonial-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.heading_size }}px;
    color: {{ block.settings.heading_color }};
    margin: 0 0 16px;
    font-weight: 600;
  }

  .ai-testimonial-subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subheading_size }}px;
    color: {{ block.settings.text_color }};
    margin: 0;
    opacity: 0.8;
  }

  .ai-testimonial-grid-wrapper-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: {{ block.settings.grid_gap }}px;
    margin-bottom: 40px;
  }

  .ai-testimonial-card-{{ ai_gen_id }} {
    background-color: {{ block.settings.card_background_color }};
    border-radius: {{ block.settings.card_border_radius }}px;
    padding:24px;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);
    transition: transform 0.3s ease, box-shadow 0.3s ease;border: 1px solid {{ block.settings.card_border_color }};
  }

  .ai-testimonial-card-{{ ai_gen_id }}:hover {
    transform: translateY(-4px);
    box-shadow: 0 8px 24px rgba(0, 0, 0, 0.15);
  }

  .ai-testimonial-stars-{{ ai_gen_id }} {
    display: flex;
    gap: 2px;
    margin-bottom: 16px;
  }

  .ai-testimonial-star-{{ ai_gen_id }} {
    width: 16px;
    height: 16px;
    color: {{ block.settings.star_color }};}

  .ai-testimonial-star-empty-{{ ai_gen_id }} {
    color: {{ block.settings.star_empty_color }};
  }

  .ai-testimonial-content-{{ ai_gen_id }} {
    font-size: {{ block.settings.testimonial_text_size }}px;
    line-height: 1.6;
    color: {{ block.settings.text_color }};
    margin-bottom: 20px;
    font-style: italic;
  }

  .ai-testimonial-author-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 12px;
  }

  .ai-testimonial-avatar-{{ ai_gen_id }} {
    width: 48px;
    height: 48px;
    border-radius: 50%;
    overflow: hidden;
    flex-shrink: 0;
    background-color: {{ block.settings.avatar_background_color }};display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-testimonial-avatar-{{ ai_gen_id }} img {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-testimonial-avatar-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: {{ block.settings.avatar_background_color }};
    color: {{ block.settings.avatar_text_color }};
    font-size:18px;
    font-weight: 600;
  }

  .ai-testimonial-author-info-{{ ai_gen_id }} {
    flex-grow: 1;
  }

  .ai-testimonial-author-name-{{ ai_gen_id }} {
    font-size: {{ block.settings.author_name_size }}px;
    font-weight: 600;
    color: {{ block.settings.heading_color }};
    margin: 0 0 4px;
  }

  .ai-testimonial-author-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.author_title_size }}px;
    color: {{ block.settings.text_color }};
    opacity: 0.7;
    margin: 0;
  }

  .ai-testimonial-empty-state-{{ ai_gen_id }} {
    text-align: center;
    padding: 60px 20px;
    color: {{ block.settings.text_color }};
    opacity: 0.6;
  }

  .ai-testimonial-empty-icon-{{ ai_gen_id }} {
    width: 64px;
    height: 64px;
    margin: 0 auto 16px;
    opacity: 0.3;
  }

  @media screen and (max-width: 768px) {
    .ai-testimonial-grid-wrapper-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 20px;
    }

    .ai-testimonial-card-{{ ai_gen_id }} {
      padding: 20px;
    }

    .ai-testimonial-title-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.heading_size }}px * 0.8);
    }

    .ai-testimonial-subtitle-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.subheading_size }}px * 0.9);
    }
  }
{% endstyle %}

<div class="ai-testimonial-grid-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-testimonial-container-{{ ai_gen_id }}">
    {% if block.settings.heading != blank or block.settings.subheading != blank %}
      <div class="ai-testimonial-header-{{ ai_gen_id }}">
        {% if block.settings.heading != blank %}
          <h2 class="ai-testimonial-title-{{ ai_gen_id }}">{{ block.settings.heading }}</h2>
        {% endif %}
        {% if block.settings.subheading != blank %}
          <p class="ai-testimonial-subtitle-{{ ai_gen_id }}">{{ block.settings.subheading }}</p>
        {% endif %}
      </div>
    {% endif %}

    {% assign has_testimonials = false %}
    {% for i in (1..30) %}
      {% assign testimonial_key = 'testimonial_' | append: i %}
      {% assign author_key = 'author_' | append: i %}
      {% assign title_key = 'author_title_' | append: i %}
      {% assign rating_key = 'rating_' | append: i %}
      {% assign avatar_key = 'avatar_' | append: i %}

      {% if block.settings[testimonial_key] != blank %}
        {% assign has_testimonials = true %}
        {% break %}
      {% endif %}
    {% endfor %}

    {% if has_testimonials %}
      <div class="ai-testimonial-grid-wrapper-{{ ai_gen_id }}">
        {% for i in (1..30) %}
          {% assign testimonial_key = 'testimonial_' | append: i %}
          {% assign author_key = 'author_' | append: i %}
          {% assign title_key = 'author_title_' | append: i %}
          {% assign rating_key = 'rating_' | append: i %}
          {% assign avatar_key = 'avatar_' | append: i %}

          {% assign testimonial_text = block.settings[testimonial_key] %}
          {% assign author_name = block.settings[author_key] %}
          {% assign author_title = block.settings[title_key] %}
          {% assign rating = block.settings[rating_key] %}
          {% assign avatar = block.settings[avatar_key] %}

          {% if testimonial_text != blank %}
            <div class="ai-testimonial-card-{{ ai_gen_id }}">
              {% if rating > 0 %}
                <div class="ai-testimonial-stars-{{ ai_gen_id }}">
                  {% for star in (1..5) %}
                    {% if star <= rating %}
                      <svg class="ai-testimonial-star-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="currentColor">
                        <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"/>
                      </svg>
                    {% else %}
                      <svg class="ai-testimonial-star-{{ ai_gen_id }} ai-testimonial-star-empty-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="currentColor">
                        <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"/>
                      </svg>
                    {% endif %}
                  {% endfor %}
                </div>
              {% endif %}

              <div class="ai-testimonial-content-{{ ai_gen_id }}">
                "{{ testimonial_text }}"
              </div>

              <div class="ai-testimonial-author-{{ ai_gen_id }}">
                <div class="ai-testimonial-avatar-{{ ai_gen_id }}">
                  {% if avatar %}
                    <img src="{{ avatar | image_url: width: 96}}" alt="{{ author_name | escape }}" loading="lazy">
                  {% else %}
                    <div class="ai-testimonial-avatar-placeholder-{{ ai_gen_id }}">
                      {% if author_name != blank %}
                        {{ author_name | slice: 0 }}
                      {% else %}
                        ?
                      {% endif %}
                    </div>
                  {% endif %}
                </div>
                <div class="ai-testimonial-author-info-{{ ai_gen_id }}">
                  {% if author_name != blank %}
                    <div class="ai-testimonial-author-name-{{ ai_gen_id }}">{{ author_name }}</div>
                  {% endif %}
                  {% if author_title != blank %}
                    <div class="ai-testimonial-author-title-{{ ai_gen_id }}">{{ author_title }}</div>
                  {% endif %}</div>
              </div>
            </div>
          {% endif %}
        {% endfor %}
      </div>
    {% else %}
      <div class="ai-testimonial-empty-state-{{ ai_gen_id }}"><div class="ai-testimonial-empty-icon-{{ ai_gen_id }}"><svg viewBox="0 0 24 24" fill="currentColor">
            <path d="M202H4c-1.1 0-2 .9-2 2v12c0 1.1.9 2 2 2h4l44 4-4h4c1.1 0 2-.9 2-2V4c0-1.1-.9-2-2-2zm-7 13.5c-.83 0-1.5-.67-1.5-1.5s.67-1.5 1.5-1.5 1.5.67 1.5 1.5-.67 1.5-1.5 1.5zm1-4.5h-2V6h2v5z"/>
          </svg>
        </div>
        <h3>Add your first testimonial</h3>
        <p>Start building trust by adding client testimonials and reviews</p>
      </div>
    {% endif %}
  </div>
</div>

