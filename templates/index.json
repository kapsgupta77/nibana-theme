            "text_4WbPEg": {
              "type": "text",
              "name": "t:names.heading",
              "settings": {
                "text": "<h2>Our Mission</h2>",
                "width": "100%",
                "max_width": "normal",
                "alignment": "center",
                "type_preset": "h3",
                "font": "var(--font-body--family)",
                "font_size": "1rem",
                "line_height": "normal",
                "letter_spacing": "normal",
                "case": "none",
                "wrap": "pretty",
                "color": "var(--color-foreground)",
                "background": false,
                "background_color": "#00000026",
                "corner_radius": 0,
