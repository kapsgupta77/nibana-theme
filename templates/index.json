    "section_4EQaYA": {
      "type": "section",
      "blocks": {
        "text_AQQxiQ": {
          "type": "text",
          "name": "t:names.heading",
          "settings": {
            "text": "<h3><strong>Frequently asked questions</strong></h3>",
            "width": "fit-content",
